        <TextBox Name="TruckBox" HorizontalAlignment="Left" Height="18" Margin="490,47,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="119"/>
        <TextBox Name="FNameTxt" IsReadOnly="True" TextAlignment="Center" HorizontalAlignment="Left" Height="23" Margin="14,21,0,0" TextWrapping="Wrap" Text="First Name" VerticalAlignment="Top" Width="119"/>
